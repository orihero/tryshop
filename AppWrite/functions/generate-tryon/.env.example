# Which AI provider to use for try-on generation: "gemini" or "fashn" (default: gemini)
TRYON_PROVIDER=gemini

# Google Gemini (Nano Banana) API key from https://aistudio.google.com/apikey
# Required when TRYON_PROVIDER=gemini
GEMINI_API_KEY=

# FASHN API key from https://app.fashn.ai (Developer API → API Keys)
# Required when TRYON_PROVIDER=fashn
FASHN_API_KEY=

# Appwrite API key with read/write access to your database (and storage if you add result uploads).
# Create in Appwrite Console: Project → API Keys.
APPWRITE_API_KEY=

# Your Appwrite database ID (same as VITE_APPWRITE_DATABASE_ID in the main app).
APPWRITE_DATABASE_ID=

# Optional. Usually injected by Appwrite as APPWRITE_FUNCTION_PROJECT_ID when the function runs in the cloud.
APPWRITE_PROJECT_ID=

# Optional. Appwrite API endpoint (default: https://cloud.appwrite.io/v1).
# Use your region if needed, e.g. https://sgp.cloud.appwrite.io/v1
APPWRITE_ENDPOINT=

# Appwrite Storage bucket ID for try-on result images.
# Same as VITE_APPWRITE_TRY_ON_BUCKET_ID in the main app.
APPWRITE_TRY_ON_BUCKET_ID=
